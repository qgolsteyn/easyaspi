{
    "presets": ["babel-preset-expo"],
    "plugins": [
        [
            "module-resolver",
            {
                "alias": {
                    "@client": "./src",
                    "@shared": "./src/shared"
                },
                "extensions": [".js", ".jsx", ".ts", ".tsx"],
                "cwd": "babelrc"
            }
        ]
    ]
}
